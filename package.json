{
  "name": "Metalify-Riot-Starter",
  "repository": "git@github.com:gitbreaker222/metalify-riot-starter.git",
  "homepage": "https://github.com/gitbreaker222/metalify-riot-starter",
  "description": "Website generator with Metalsmith, Netlify / NetlifyCMS and Riot JS",
  "bin": {
    "metalsmith": "bin/metalsmith",
    "_metalsmith": "bin/_metalsmith"
  },
  "scripts": {
    "pretest": "npm install",
    "test": "mocha $HARMONY_OPTS",
    "pretest-debug": "npm install",
    "test-debug": "mocha debug",
    "precoverage": "npm install",
    "coverage": "node $HARMONY_OPTS ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha",
    "precoveralls": "npm run coverage",
    "coveralls": "coveralls < coverage/lcov.info"
  },
  "dependencies": {
    "absolute": "0.0.1",
    "babel-eslint": "~7.2.3",
    "chalk": "^1.1.3",
    "clone": "^1.0.2",
    "co-fs-extra": "^1.2.1",
    "commander": "^2.6.0",
    "eslint": "^4.3.0",
    "eslint-plugin-html": "^3.1.1",
    "gray-matter": "^2.1.0",
    "has-generators": "^1.0.1",
    "is": "^3.1.0",
    "is-utf8": "~0.2.0",
    "metalsmith": "^2.1.0",
    "metalsmith-concat": "^6.0.0",
    "metalsmith-layouts": "^1.4.1",
    "metalsmith-markdown": "0.2.1",
    "metalsmith-move-up": "^1.0.0",
    "metalsmith-sass": "^1.4.0",
    "prismic-dom": "^1.2.8",
    "prismic-javascript": "^1.2.0",
    "pug": "^2.0.0-rc.3",
    "recursive-readdir": "^2.1.0",
    "rimraf": "^2.2.8",
    "riot": "^3.6.1",
    "stat-mode": "^0.2.0",
    "thunkify": "^2.1.2",
    "unyield": "0.0.1",
    "ware": "^1.2.0",
    "win-fork": "^1.1.1"
  },
  "devDependencies": {
    "assert-dir-equal": "~1.0.1",
    "co-mocha": "^1.0.1",
    "coveralls": "^2.11.6",
    "istanbul": "^0.4.1",
    "mocha": "^3.0.2"
  },
  "keywords": [
    "static",
    "site",
    "website",
    "blog",
    "generator",
    "boilerplate",
    "metalsmith",
    "netlify",
    "netlifyCMS",
    "riot",
    "riotjs"
  ]
}
